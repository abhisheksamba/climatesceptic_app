<%= flash[:notice] %>
<table>
    <thead>
        <tr>
            <th> Id </th>
            <th> Date </th>
            <th> CCSI </th>
            <th> Democrat Statements For CC</th>
            <th> Republican Statements Against CC</th>
            <th> House Hearings on CC </th>
            <th> Senate Hearings on CC </th>
            <th> LCV Dem Pro-CC Score </th>
            <th> LCV Republican Anti-CC Score</th>
        </tr>
    </thead>
    <tbody>
        <% @elite_cues.each do |n| %>
        <tr>
            <td> <%= n.id %></td>
            <td> <%= n.Date %></td>
            <td> <%= n.CCSI %></td>
            <td> <%= n.Democrat_Statements %></td>
            <td> <%= n.Republican_Statements %></td>
            <td> <%= n.House_Hearings %></td>
            <td> <%= n.Senate_Hearings %></td>
            <td> <%= n.LCV_Dem %></td>
            <td> <%= n.LCV_Rep %></td>
        </tr>
    <% end %>
    </tbody>
</table>
<div>
    <h3> Import a CSV File</h3>
    <%= form_tag import_ccsi_elite_index_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import CSV" %>
    <% end %>
</div>
