<%= flash[:notice] %>
<table>
    <thead>
        <tr>
            <th> Id </th>
            <th> Date </th>
            <th> CCSI </th>
            <th> Unemployment Rate</th>
            <th> GDP Growth (%change)</th>
            <th> Price of Oil </th>
            <th> Industrial Production Index </th>
            <th> Recession Dates </th>
            <th> Total Nonfarm Payrolls (%change) </th>
        </tr>
    </thead>
    <tbody>
        <% @recessions.each do |n| %>
        <tr>
            <td> <%= n.id %></td>
            <td> <%= n.Date %></td>
            <td> <%= n.CCSI %></td>
            <td> <%= n.Unemployment %></td>
            <td> <%= n.GDPGrowth %></td>
            <td> <%= n.Oil_Price %></td>
            <td> <%= n.IPI %></td>
            <td> <%= n.Recession_Dates %></td>
            <td> <%= n.Nonfarm_Payrolls %></td>
        </tr>
    <% end %>
    </tbody>
</table>
<div>
    <h3> Import a CSV File</h3>
    <%= form_tag import_econ_recession_index_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import CSV" %>
    <% end %>
</div>
